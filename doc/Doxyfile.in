PROJECT_NAME           = "StarVine"
OUTPUT_DIRECTORY       = @CMAKE_CURRENT_BINARY_DIR@
STRIP_FROM_PATH        = @PACKAGE_SOURCE_DIR@ \
                         @PACKAGE_BINARY_DIR@
                         @PACKAGE_SOURCE_DIR@ \
                         @PACKAGE_BINARY_DIR@
IMAGE_PATH             = @PACKAGE_SOURCE_DIR@/docs/images
FILE_PATTERNS          = *.h \
                         *.c \
                         *.cpp \
                         *.hpp \
                         *.py \
                         *.md
RECURSIVE              = YES
USE_MDFILE_AS_MAINPAGE = @PACKAGE_SOURCE_DIR@/README.md
DISABLE_INDEX          = NO
GENERATE_TREEVIEW      = YES
WARN_IF_UNDOCUMENTED   = NO
